{
  "name": "md2slack",
  "version": "1.1.4",
  "description": "Command line tool that takes markdown from stdin and writes Slack markdown to stdout",
  "source": "src/index.ts",
  "main": "dist/index.mjs",
  "scripts": {
    "test": "jest",
    "prepare": "husky install",
    "build": "parcel build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rbutera/markdown-to-slack.git"
  },
  "keywords": [
    "markdown",
    "slack",
    "clt",
    "cli"
  ],
  "author": "rbutera",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/rbutera/markdown-to-slack/issues"
  },
  "homepage": "https://github.com/rbutera/markdown-to-slack#readme",
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=18.7.0"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.2",
    "@types/jest": "^29.4.2",
    "@types/node": "^18.15.3",
    "@types/yargs": "^17.0.32",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-xo": "^0.43.1",
    "eslint-config-xo-typescript": "^0.56.0",
    "eslint-plugin-functional": "^5.0.6",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unicorn": "^46.0.0",
    "eslint-plugin-you-dont-need-lodash-underscore": "^6.12.0",
    "husky": "^8.0.1",
    "jest": "^29.5.0",
    "jest-extended": "^3.2.4",
    "lint-staged": "^13.0.1",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.7.0",
    "prettier": "^2.6.2",
    "remark": "^14.0.2",
    "rimraf": "^4.4.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --cache --fix --quiet",
      "jest --runInBand --findRelatedTests --passWithNoTests"
    ],
    "*.{js,ts,css,md}": "prettier --write"
  },
  "bin": "./dist/index.mjs",
  "dependencies": {
    "date-fns": "^2.29.2",
    "slackify-markdown": "^4.3.1",
    "yargs": "^17.5.1"
  }
}
