{
  "name": "md2slack",
  "version": "0.0.1",
  "description": "Command line tool that takes markdown from stdin and writes Slack markdown to stdout",
  "main": "index.js",
  "scripts": {
    "test": "xo && jest",
    "prepare": "husky install",
    "build": "tsc --project tsconfig.build.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rbutera/markdown-to-slack.git"
  },
  "keywords": [
    "markdown",
    "slack",
    "clt",
    "cli"
  ],
  "author": "rbutera",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/rbutera/markdown-to-slack/issues"
  },
  "homepage": "https://github.com/rbutera/markdown-to-slack#readme",
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.2",
    "@types/jest": "^28.1.1",
    "@types/node": "^17.0.41",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-xo": "^0.41.0",
    "eslint-config-xo-typescript": "^0.51.1",
    "eslint-plugin-functional": "^4.2.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unicorn": "^42.0.0",
    "eslint-plugin-you-dont-need-lodash-underscore": "^6.12.0",
    "husky": "^8.0.1",
    "jest": "^28.1.1",
    "jest-extended": "^2.0.0",
    "lint-staged": "^13.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "remark": "^14.0.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.4",
    "ts-node": "^10.8.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.3"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --cache --fix --quiet",
      "jest --runInBand --findRelatedTests"
    ],
    "*.{js,ts,css,md}": "prettier --write"
  },
  "bin": {
    "md2slack": "./dist/index.js"
  },
  "packageManager": "yarn@3.2.1",
  "dependencies": {
    "slackify-markdown": "^4.3.1"
  }
}
